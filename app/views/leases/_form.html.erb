<%= form_for @lease do |f| %>
  <div class="form-group">
    <%= f.label :tenant_id %>
    <br />
    <%= f.text_field :tenant_id, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :apartment_id %>
    <br />
    <%= f.text_area :apartment_id, class: 'form-control' %>
  </div>
  <div class="text-center"><%= f.submit "Submit", class: "btn btn-danger"%></div>
<% end %>